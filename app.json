{
  "expo": {
    "name": "PetConnect",
    "slug": "PetConnect",
    "version": "1.1.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/images/StartScreenImage.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/StartScreenImage.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to the camera",
        "NSPhotoLibraryUsageDescription": "This app needs access to the photo library",
        "NSPhotoLibraryAddUsageDescription": "This app needs permission to save photos"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/StartScreenImage.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/images/StartScreenImage.png",
      "package": "com.pki801.PetConnect"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "280deeb2-6545-40ac-a8ca-70013b78f65e"
      }
    }
  }
}
